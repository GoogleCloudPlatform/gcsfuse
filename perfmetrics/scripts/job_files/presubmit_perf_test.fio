[global]
ioengine=libaio
direct=1
fadvise_hint=0
verify=0
rw=read
bs=1M
iodepth=64
invalidate=1
ramp_time=1s
runtime=10s
time_based=1
nrfiles=1
thread=1
openfiles=1
fsync=1
group_reporting=1
allrandrepeat=1
filename_format=$jobname.$jobnum.$filenum

[256kb_read]
stonewall
bs=16k
directory=gcs/256kb
filesize=256k
rw=read
numjobs=40

[256kb_write]
stonewall
bs=16k
directory=gcs/256kb
filesize=256k
rw=write
numjobs=40

[256kb_randread]
stonewall
bs=16k
directory=gcs/256kb
filesize=256k
rw=randread
numjobs=40

[256kb_randwrite]
stonewall
bs=16k
directory=gcs/256kb
filesize=256k
rw=randwrite
numjobs=40

[50mb_read]
stonewall
directory=gcs/50mb
filesize=50M
rw=read
numjobs=40

[50mb_write]
stonewall
directory=gcs/50mb
filesize=50M
rw=write
numjobs=40

[50mb_randread]
stonewall
directory=gcs/50mb
filesize=50M
rw=randread
numjobs=40

[50mb_randwrite]
stonewall
directory=gcs/50mb
filesize=50M
rw=randwrite
numjobs=40

[1gb_read]
stonewall
directory=gcs/1gb
filesize=1G
rw=read
numjobs=40

[1gb_write]
stonewall
directory=gcs/1gb
filesize=1G
rw=write
numjobs=40

[1gb_randread]
stonewall
directory=gcs/1gb
filesize=1G
rw=randread
numjobs=40

[1gb_randwrite]
stonewall
directory=gcs/1gb
filesize=1G
rw=randwrite
numjobs=40
