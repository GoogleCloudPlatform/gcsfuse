; -- use nrfiles and rw to CLI args to control readtype and number of files --
[global]
ioengine=libaio
direct=1
fadvise_hint=0
iodepth=64
invalidate=1
ramp_time=10s
runtime=60s
time_based=1
nrfiles=1
thread=1
openfiles=1
group_reporting=1
allrandrepeat=1
numjobs=40
filename_format=$jobname.$jobnum.$filenum

[1KB]
directory=gcs/100M
bs=1M
filesize=100M
rw=read
