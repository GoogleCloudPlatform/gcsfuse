; -- use nrfiles and rw to CLI args to control readtype and number of files --
[global]
ioengine=libaio
direct=0
fadvise_hint=0
iodepth=64
invalidate=1
nrfiles=${NRFILES}
thread=1
openfiles=1
group_reporting=1
create_serialize=0
allrandrepeat=0
file_service_type=random
numjobs=${NUMJOBS}
filename_format=$jobname.$jobnum/$filenum

[Workload]
directory=${DIR}
bs=${BLOCK_SIZE}
filesize=${FILE_SIZE}
rw=${READ_TYPE}
